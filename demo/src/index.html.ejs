  <style>
    * {
      box-sizing: border-box;
    }

    html,
    body,
    #app {
      width: 100%;
      height: 100%;
      padding: 0;
      margin: 0;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      position: absolute;
      left: 0;
      top: 0;
      overflow: hidden;
    }

    img.img_catagory {
      /* height: 40px; */
      width: 40px !important;
    }

    .text-center {
      text-align: center;
    }

    aside.toolbar {
      /* display: none; */
      display: flex;
    }

    aside.toolbar div {
      margin: 0px 20px;
    }

    /* .toolbar:nth-child(2) {
        display: none !important;
      } */
    aside.sidebar {
      /* display: none !important; */
    }

    .abc:nth-child(1) {
      top: 50px !important;
    }

    /* width */
    ::-webkit-scrollbar {
      width: 5px;
    }

    /* Track */
    ::-webkit-scrollbar-track {
      background: #f1f1f1;
    }

    /* Handle */
    ::-webkit-scrollbar-thumb {
      background: #888;
    }

    /* Handle on hover */
    ::-webkit-scrollbar-thumb:hover {
      background: #555;
    }

    nav.react-contextmenu.menu.react-contextmenu--visible {
      background-color: #202124;
      z-index: 1;
      padding: 10px;
      border-radius: 10px;
    }

    aside.sidebar {
      height: auto !important;
    }


